package diziler;

public class diziler {

	public static void main(String[] args) {
		String[] ogrenciler = new String[4];
		ogrenciler[0] = "Umut";
		ogrenciler[1] = "Kamil";
		ogrenciler[2] = "Sinan";
		ogrenciler[3] = "Yakup";

		for (int i =0; i<ogrenciler.length; i++)
		{
			System.out.println("Öğrenciler :" + ogrenciler[i]);
		}
	}

}
